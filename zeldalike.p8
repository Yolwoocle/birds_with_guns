--mouse
function mouse_x_y()
	poke(0x5f2d, 1)
	mouse_x=stat(32)
	mouse_y=stat(33)
end

function get_traj(x_satr,y_start,x_end,y_end)
	angle=atan2(x_end-x_satr, y_end-y_start)
	return {x = cos(angle),y = sin(angle)}
end

function draw_mouse()
	spr(2,mouse_x,mouse_y)
end

